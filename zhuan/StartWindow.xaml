<Window x:Class="zhuan.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StartWindow" Height="720" Width="1280" Background="White" KeyDown="keydown">
    <Grid>
        <Image Source="images/start.png" Stretch="Fill" />
        <Image x:Name="img1" Height="5" Margin="433,432,789,253" Width="50" Source="images/me.png" Stretch="Fill" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <TransformGroup>
                    <RotateTransform x:Name="zhuan1" Angle="0"/>
                    <TranslateTransform x:Name="dong1" Y="0"/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="img2" Height="5" Margin="826,432,396,253" Width="50" Source="images/me.png" Stretch="Fill" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <TransformGroup>
                    <RotateTransform x:Name="zhuan2" Angle="0"/>
                    <TranslateTransform x:Name="dong2" Y="0"/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>

    </Grid>
</Window>
